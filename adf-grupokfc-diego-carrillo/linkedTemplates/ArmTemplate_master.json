{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-grupokfc-diego-carrillo"
		},
		"ls_fileSystem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_fileSystem'"
		},
		"ls_kfc_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_kfc_datalake'"
		},
		"ls_fileSystem_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\diego.carrillo\\datafactory\\"
		},
		"ls_fileSystem_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "kfc\\diego.carrillo"
		},
		"ls_kfc_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsgrupokfcadf.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-grupokfc-diego-carrillo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_kfc_datalake_accountKey": {
						"value": "[parameters('ls_kfc_datalake_accountKey')]"
					},
					"ls_fileSystem_password": {
						"value": "[parameters('ls_fileSystem_password')]"
					},
					"ls_kfc_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_kfc_datalake_properties_typeProperties_url')]"
					},
					"ls_fileSystem_properties_typeProperties_host": {
						"value": "[parameters('ls_fileSystem_properties_typeProperties_host')]"
					},
					"ls_fileSystem_properties_typeProperties_userId": {
						"value": "[parameters('ls_fileSystem_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}