{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "adf-grupokfc-diego-carrillo"
		},
		"ls_fileSystem_password": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"password\"de \"ls_fileSystem\""
		},
		"ls_kfc_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accountKey\"de \"ls_kfc_datalake\""
		},
		"ls_sql_local_connectionString": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"connectionString\"de \"ls_sql_local\""
		},
		"ls_sql_local_password": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"password\"de \"ls_sql_local\""
		},
		"ls_azure_sql_database_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "connection-string"
		},
		"ls_fileSystem_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\diego.carrillo\\datafactory\\"
		},
		"ls_fileSystem_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "kfc\\diego.carrillo"
		},
		"ls_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-grupokfc-adf.vault.azure.net/"
		},
		"ls_kfc_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsgrupokfcadf.dfs.core.windows.net"
		},
		"ls_sql_local_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "kfc\\diego.carrillo"
		},
		"SC_CalendarizacionCargaInicial_properties_PL_CARGA_FECHAS_parameters_Desde": {
			"type": "string",
			"defaultValue": "20220201"
		},
		"SC_CalendarizacionCargaInicial_properties_PL_CARGA_FECHAS_parameters_Hasta": {
			"type": "string",
			"defaultValue": "20220208"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-grupokfc-diego-carrillo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_kfc_datalake_accountKey": {
						"value": "[parameters('ls_kfc_datalake_accountKey')]"
					},
					"ls_fileSystem_password": {
						"value": "[parameters('ls_fileSystem_password')]"
					},
					"ls_sql_local_connectionString": {
						"value": "[parameters('ls_sql_local_connectionString')]"
					},
					"ls_sql_local_password": {
						"value": "[parameters('ls_sql_local_password')]"
					},
					"ls_keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_keyvault_properties_typeProperties_baseUrl')]"
					},
					"ls_kfc_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_kfc_datalake_properties_typeProperties_url')]"
					},
					"ls_azure_sql_database_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_azure_sql_database_properties_typeProperties_connectionString_secretName')]"
					},
					"ls_fileSystem_properties_typeProperties_host": {
						"value": "[parameters('ls_fileSystem_properties_typeProperties_host')]"
					},
					"ls_fileSystem_properties_typeProperties_userId": {
						"value": "[parameters('ls_fileSystem_properties_typeProperties_userId')]"
					},
					"ls_sql_local_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_local_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-grupokfc-diego-carrillo_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SC_CalendarizacionCargaInicial_properties_PL_CARGA_FECHAS_parameters_Desde": {
						"value": "[parameters('SC_CalendarizacionCargaInicial_properties_PL_CARGA_FECHAS_parameters_Desde')]"
					},
					"SC_CalendarizacionCargaInicial_properties_PL_CARGA_FECHAS_parameters_Hasta": {
						"value": "[parameters('SC_CalendarizacionCargaInicial_properties_PL_CARGA_FECHAS_parameters_Hasta')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-grupokfc-diego-carrillo_ArmTemplate_0')]"
			]
		}
	]
}